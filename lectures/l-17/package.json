{
  "name": "l-17",
  "version": "1.0.0",
  "description": "l-17 - A modern web application using Tailwind CSS and Drizzle ORM",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "NODE_ENV=${NODE_ENV:-development} vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "npm run clean && npm run build && wrangler deploy",
    "migrate:new": "drizzle-kit generate",
    "migrate:dev": "wrangler d1 migrations apply DB --local",
    "migrate:prod": "wrangler d1 migrations apply DB --remote",
    "db:seed": "npm run worker:run ./src/db/seed.ts",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "better-auth": "^1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.4",
    "lucide-react": "^0.539.0",
    "nanoid": "^5.1.5",
    "ohash": "^2.0.11",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rwsdk": "0.2.0-alpha.16",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "drizzle-kit": "^0.31.4",
    "jsdom": "^26.1.0",
    "tsx": "^4.20.4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "wrangler": "^4.30.0"
  }
}

    "react": "19.2.0-canary-3fb190f7-20250908",
    "react-dom": "19.2.0-canary-3fb190f7-20250908",
    "react-server-dom-webpack": "19.2.0-canary-3fb190f7-20250908",
    "rwsdk": "1.0.0-alpha.5",
